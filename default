server {
	listen 80 default_server;
	listen [::]:80 default_server;
	
	root ./frontend/
	
	# SSL configuration
	listen 443 ssl default_server;
	listen [::]:443 ssl default_server;
	
	server_name _;
    ssl_certificate /etc/nginx/nginx.crt;
    ssl_certificate_key /etc/nginx/nginx.key;
	
	location / {
		#proxy_pass	http://localhost:1313/;
	}
}